{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "mocha": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "react/no-multi-comp": 0,
    "react/prefer-stateless-function": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "comma-dangle": 0,
    "indent": [ 2, 2, { "SwitchCase" : 1 } ],
    "no-console": 0,
    "no-alert": 0
  },
  "plugins": [
    "react", "import"
  ],
  "globals": {
    "expect": true
  }
}
